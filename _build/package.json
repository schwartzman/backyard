{
	"name": "backyard-build",
	"description": "backyard cafe build module",
	"private": "true",
	"license": "MIT",
	"repository": {},
	"dependencies": {
		"autoprefixer": "^8.6.4",
		"node-sass": "^4.13.1",
		"postcss-cli": "^5.0.1"
	},
	"scripts": {
		"sass": "node-sass --output-style expanded -o ../library/css/ ../library/scss/",
		"postsass": "postcss ../library/css/* --use autoprefixer --no-map --replace",
        "w:s": "watch 'npm run sass' ../library/scss/"
	}
}
